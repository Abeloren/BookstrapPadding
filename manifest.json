{
  "manifest_version": 2,

  "name": "Format Bookstrap",
  "description": "Formats layout of bookstrap.hackreactor.com",
  "version": "1.0",

  "browser_action": {
    "default_title": "Format Bookstrap!",
    "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["*://bookstrap.hackreactor.com/*"],
      "js": ["run.js"]
    }
  ],

  "permissions": [
    "activeTab", "tabs"
  ]
}

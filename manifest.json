{
  "manifest_version": 2,

  "name": "Format Bookstrap",
  "description": "Improves layout of bookstrap.hackreactor.com",
  "version": "1.2",

  "icons": {
    "128": "icon_151px.png"
  },

  "content_scripts": [
    {
      "matches": ["*://bookstrap.hackreactor.com/*"],
      "js": ["run.js"]
    }
  ],

  "permissions": [
    "activeTab", "tabs"
  ]
}
